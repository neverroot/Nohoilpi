         <th aria-label="Date" data-stat="game_date" class=" sort_default_asc center">Date</th>
         <th aria-label="Game Time, Eastern" data-stat="gametime" class=" sort_default_asc center" data-tip="Game Time, Eastern">Time</th>
         <th aria-label="Visitor" data-stat="winner" class=" sort_default_asc center">Visitor</th>
         <th aria-label="game_location" data-stat="game_location" class=" sort_default_asc center"></th>
